# Server Configuration
PORT=3000
API_KEY=your-secret-api-key-here
MODEL_NAME=lumo

# Logging Configuration
# Levels: trace, debug, info, warn, error, fatal
LOG_LEVEL=debug

# Browser Configuration
CHATBOX_URL=https://lumo.proton.me/u/4/

# Remote Browser Connection (REQUIRED)
# Connects to remote browser via Chrome DevTools Protocol (CDP)
CDP_ENDPOINT=ws://browser:9222

# Chatbox Selectors (customize for your target chatbox)
SELECTOR_INPUT=.tiptap
SELECTOR_SEND_BUTTON=button.button-solid-norm
SELECTOR_MESSAGES=.assistant-msg-container
SELECTOR_COMPLETION_INDICATOR=use[*|href="#ic-thumb-up"]

# Response Timeout Configuration (in milliseconds)
RESPONSE_TIMEOUT_WITH_TEXT=2000
RESPONSE_TIMEOUT_EMPTY=20000
