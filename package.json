{
  "name": "lumo-bridge",
  "version": "1.0.0",
  "description": "OpenAI-compatible API bridge for web chatboxes using Playwright",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev:server": "tsx watch src/server.ts",
    "dev:cli": "tsx src/cli.ts",
    "dev:server:debug": "tsx watch --inspect=0.0.0.0:9229 src/server.ts",
    "dev:cli:debug": "tsx --inspect=0.0.0.0:9229 src/cli.ts",
    "build": "tsc && tsc-alias",
    "server": "node dist/server.js",
    "cli": "node dist/cli.js",
    "clean": "rm -rf dist",
    "dev:auth": "tsx src/auth/authenticate.ts",
    "auth": "node dist/auth/authenticate.js",
    "auth-status": "tsx src/auth/status.ts",
    "sync-upstream": "./scripts/sync-upstream.sh"
  },
  "keywords": [
    "openai",
    "api",
    "playwright",
    "chatbox",
    "bridge"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/lodash": "^4.17.23",
    "@types/toposort": "^2.0.7",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "js-yaml": "^4.1.1",
    "json-stable-stringify": "^1.3.0",
    "lodash": "^4.17.23",
    "openpgp": "^6.0.0",
    "p-queue": "^8.0.1",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "playwright": "^1.41.0",
    "toposort": "^2.0.2",
    "uuid": "^9.0.1",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.5",
    "@types/uuid": "^9.0.8",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
